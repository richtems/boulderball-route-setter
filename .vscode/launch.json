{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Backend",
      "request": "launch",
      "command": "npm run start:debug",
      "cwd": "${workspaceFolder}\\boulderball-route-setter-backend"
    },
    {
      "type": "node-terminal",
      "name": "Frontend",
      "request": "launch",
      "command": "npm run start",
      "cwd": "${workspaceFolder}\\boulderball-route-setter-frontend"
    }
  ],
  "compounds": [
    {
      "name": "Backend/Frontend",
      "configurations": ["Backend", "Frontend"],
      "stopAll": true
    }
  ]
}
